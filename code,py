
    #  bolib beruvchi code
from math import sqrt
number = list(range(1,11))
ildizlarr = list(map(sqrt,number))
print(ildizlarr)





    #    kvadrat code
kvadratlar = []
for i in number:
    kvadratlar.append(i*i)
    # kvadratlar.append(i**2)
print(kvadratlar)



    #    qoshish + code
birinchi = [1,2,3]
ikkinchi = [4,5,6]
uchinchi = list(map(lambda i, o : i + o, birinchi, ikkinchi))
print(uchinchi)






    #    LSTichidagilarni kotta qilib beradi code 
names = ['shukrullo', 'doston', 'norbuta', 'azamat']
print(list(map(lambda i: i.upper(), names)))





        #  LST bu cod lst ichidagi bosh xarifini a bolsa dasturda chiqazadi a ismlarni
ismlar = ['shukrullo', 'doston', 'norbuta', 'azamat', 'ali', 'abbos']
ism = list(filter(lambda x: x.startswith('a'), ismlar))
print(ism)




    #    Kopaytiruvchi code 
daraja = lambda a, b: a ** b
print(daraja(2, 4))





    #    kvadrat javobi code 
def daraja(n):
    return lambda x: x ** n
kv = daraja(10)  
print(kv(2))





        # qoshuvchi code new variyant 
a, b = map(int, input(">>> ").split())
print(a+b)






    # kopaytiruvch code number ga yoziladi mashi codaa yozilgan  
number = lambda x: x * 10
result = number(23)
print(result)




